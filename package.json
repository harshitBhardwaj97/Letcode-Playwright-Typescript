{
  "name": "letcode-playwright-typescript",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test:ui": "npx playwright test --ui",
    "test:chrome": "npx playwright test --project=chromium",
    "test:debug": "npx playwright test --debug",
    "test:headed": "npx playwright test --headed",
    "post----test": "npx playwright show-report",
    "dot:report": "npx playwright test --reporter=dot",
    "line:report": "npx playwright test --reporter=line",
    "allure": "allure generate ./allure-results --clean && allure open ./allure-report",
    "report": "playwright show-report playwright-report",
    "format": "npx prettier --write ."
  },
  "keywords": [],
  "author": "Harshit Bhardwaj",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@types/node": "^22.13.9",
    "allure-playwright": "^3.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "winston": "^3.17.0"
  }
}
